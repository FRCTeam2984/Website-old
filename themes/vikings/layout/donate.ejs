<div class="index article article-entry extra-donate">
  <% var split = page.content.split("<partbreak></partbreak>")  %>
  <%- split[0] %>
  <div class="donate-wrapper">
    <div class="donate-left-wrapper">
      <%- split[1] %>
    </div>
    <div class="donate-right-wrapper">
      <%- split[2] %>
    </div>
  </div>
  <%- split[3] %>
  <div class="donate-center">
    <%- split[4] %>
  </div>
</div>
